{
	"compilerOptions": {
		"target": "es2017",
		"module": "commonjs",
		"moduleResolution": "node",
		"outDir": "dist",
		"rootDir": "src",
		"lib": ["ES2020", "ES2020.String"],
		"strict": true,
		"declaration": true,
		"removeComments": true,
		"resolveJsonModule": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"strictPropertyInitialization": false,
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"sourceMap": true,
		"incremental": true,
		"baseUrl": "src"
	},
	"exclude": [
		"serverless.ts",
		"serverless.js",
		"serverless.js.map",
		"serverless.d.ts"
	]
}
